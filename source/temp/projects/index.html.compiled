  var fillWindow = function() {
    var $body = $('body');
    var windowH = $window.height();
    if ($body.height() < windowH) {
      $body.height(windowH);
    } else {
      $body.height('auto');
    }
  };
  $window.resize(fillWindow);
  fillWindow();

